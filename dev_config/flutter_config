
# Flutter Enviroment.
#export FLUTTER_HOME=$HOME/Soft/flutter_1.17.5
export FLUTTER_HOME=$HOME/Soft/fvm/default
export PATH=$FLUTTER_HOME/bin:$PATH

# Flutter proxy for CN.
export FLUTTER_STORAGE_BASE_URL=https://storage.flutter-io.cn
export PUB_HOSTED_URL=https://pub.flutter-io.cn
export FLUTTER_GIT_URL=https://gitee.com/mirrors/Flutter.git

export FVM_HOME=$HOME/Soft/fvm
export FVM_GIT_CACHE=https://gitee.com/mirrors/Flutter.git
export PATH="$PATH:$HOME/.pub-cache/bin"
